{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "io.github.ilikeyourhat.fmnw.**"
      ],
      "enabled": false
    },
    {
      "matchPackagePatterns": [
        "org.jetbrains.kotlin.*",
        "com.google.devtools.ksp"
      ],
      "groupName": "kotlin"
    }
  ],
  "automerge": true
}
